  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@supabase/supabase-js": "^2.49.8",
    "dotenv": "^16.6.1",
    "pg": "^8.16.3"
  }
}
